package first_project;

import java.utill.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//사용자에게 뭘 입력할지 알려주자.
		System.out.println("이름, 나이, 키, 결혼 여부를 입력해주세요.");
		
		//입력받을 준비
		//scanner은 객체
		java.util.Scanner scanner = new Scanner(System.in);
		
		//입력한 값들을 저장해 줄 공간을 만들자.
		//string형으로 받자. 변수 name //next 메소드 사용.
		String name = scanner.next();
		//nextInt는 공백 이전의 숫자를 읽음?
		int age = scanner.nextInt();
		double height = scanner.nextDouble();
		//boolean은 true or false
		boolean married = scanner.nextBoolean();
		
		//입력한 값들이 잘 담겼는지 확인
		System.out.println(name);
		System.out.println(age);
		System.out.println(height);
		System.out.println(married);
		
		//scanner 닫아주기
		scanner.close();
		
		
		
		
		
		
		System.out.print("hello");
		
		boolean logic = true;
		
		char ch = 'A';
		
		int number = 2135;
		
		double number2 = 5.6;
		
		//출력은 print와 println
		System.out.println(logic);
		System.out.println(ch);
		
		//string 선언
		String str = "rlaalswl";
		String str2 = new String("rlaalswl");
		
	}

}

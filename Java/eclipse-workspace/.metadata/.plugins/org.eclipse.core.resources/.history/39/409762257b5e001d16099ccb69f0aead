		//정수 integer를 담겠다.
		ArrayList<Integer> arrayList1 = new ArrayList<>();
		ArrayList<Integer> arrayList2 = new ArrayList<>();
		//i는 0부터 9까지
		for(int i=0; i<10; i++) {
			arrayList2.add(i);
